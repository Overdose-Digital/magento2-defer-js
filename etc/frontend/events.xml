<?xml version="1.0" ?>
<config xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:noNamespaceSchemaLocation="urn:magento:framework:Event/etc/events.xsd">
    <event name="controller_front_send_response_before">
        <observer instance="Overdose\MagentoOptimizer\Observer\Frontend\Http\ResponseSendBeforeOptimizeJS"
                  name="od_optimizer_defer_js"/>
        <observer instance="Overdose\MagentoOptimizer\Observer\Frontend\Http\ResponseSendBeforeLoadDelayJs"
                  name="od_optimizer_load_delay_js"/>
        <observer instance="Overdose\MagentoOptimizer\Observer\Frontend\Http\ResponseSendBeforeRemoveUrl"
                  name="od_optimizer_remove_url"/>
        <observer instance="Overdose\MagentoOptimizer\Observer\Frontend\Http\ResponseSendBeforeLazyLoadImage"
                  name="od_optimizer_lazy_load_image"/>
    </event>
</config>
